<!DOCTYPEE html>
<html>
<head>
{% block head %}
<link rel="stylesheet" href="/CSS/style.css" />
<title>{% block title %}jrothert{% endblock %}</title>
{% endblock %}
</head>
<body>
	<div id="wrapper">
		<div id="headerwrapper">
			<div id="header">							
				<ul class="header">
					<li> <img src="/Pics/GG.png" height="80px"/></li>			
					{% if email is defined %}	
					<li class="right"><a
						href="/logout/{{ _SESSION.get("token") }}">Logout</a></li>
					{% else %}
					<li class="right"><a href="/login">Login</a></li>
					<li class="right"><a href="/register">Register</a></li>
					{% endif %}
				</ul>
			</div>
		</div>
		<div id="content">
			{% block content %}
			{% endblock %}
		</div>
		<div id="footer">
			{% block footer %}
			{% endblock %}
		</div>
	</div>
</body>
</html>